{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/*.db": true
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "python.analysis.completeFunctionParens": true,
    "editor.bracketPairColorization.enabled": true
}